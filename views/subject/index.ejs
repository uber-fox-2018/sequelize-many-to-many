<!doctype html>
<html lang="en">
  <%- include('../partials/head', { title: 'Data Subject' }) %>
  <body>
    <%- include('../partials/navbar') %>
    <br><br><br>

    <div class="container">
      <div class="row">
        <div class="col-lg-12">

          <table class="table">
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">Subject Name</th>
                <th scope="col">Teachers</th>
                <th scope="col">Actions</th>
              </tr>
            </thead>
            <tbody>
              <% data.forEach(data => { %>
                <tr>
                  <td><%= data.id %></td>
                  <td><%= data.subject_name %></td>
                  <td>
                    <ol>
                      <li>Ari Supriatna</li>
                      <li>Ridho Septian Ramadhan</li>
                    </ol>
                  </td>
                  <td>
                    <a href="/subjects/<%= data.id %>/enrolled-students" class="btn btn-primary btn-sm">Enrolled Students</a>
                  </td>
                </tr>
              <% }) %>
            </tbody>
          </table>
          
        </div>
      </div>
    </div>

    <%- include('../partials/script') %>
  </body>
</html>