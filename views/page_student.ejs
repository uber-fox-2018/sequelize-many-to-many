<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>list_students</title>
</head>
<body>
    <div class="container">
        <table border="1">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Action</th>
                    <th>Options</th>
                </tr>
            </thead>
            <tbody>
                <% student.forEach( (dataStudent) => { %>
                    <tr>
                        <td><%= dataStudent.id %></td>
                        <td><%= dataStudent.first_name %></td>
                        <td><%= dataStudent.last_name %></td>
                        <td><%= dataStudent.email %></td>
                        <td><a href="subject/<%= dataStudent.id %>/add-subject">Add Subject</a></td>
                        <td>
                            <a href="/student/<%= dataStudent.id  %>/update" title="<%= dataStudent.id%>"><button>EDIT</button></a>
                            <a href="/student/<%= dataStudent.id  %>/delete" title="<%= dataStudent.id%>"><button>DELETE</button></a>
                        </td>
                    </tr>
                <% })%>
            </tbody>
        </table>
    </div>
</body>
</html>