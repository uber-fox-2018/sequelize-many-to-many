<%- include('../partials/header.ejs') %>
  <br>
    <table>
      <thead>
      <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
        <th colspan = "2">Options</th>
      </tr>
    </thead>
    <tbody>
      <% students.forEach(student => { %>
      <tr>
        <td><%= student.id%></td>
        <td><%= student.first_name%></td>
        <td><%= student.last_name%></td>
        <td><%= student.email%></td>
        <td><a href="/students/<%= student.id %>/add-subject">Add Subject</a></td>
        <td><a href="/students/edit/<%= student.id %>">Edit</a></td>
        <td><a href="/students/delete/<%= student.id %>">Delete</a></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  <br>
  <br>
  <h3><a href="/students/add">Add Student</a></h3>
  </body>
</html>