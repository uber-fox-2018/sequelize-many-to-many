<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <title>Document</title>
</head>
<body>
    <div clas="container">
        <h1 class="text-center">Students Data</h1>
        <a href="/" class="text-center"><p>Home Page</p></a>
        <a href="/student/addstudent" class="text-center"><p>Add Students Data</p></a>
        <table class="table table-hove">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Actions</th>
                    <th>Options</th>
                </tr>
            </thead>
            <tbody>
                <% dataStudent.forEach((dataStudent) => {%>
                    <tr>
                        <td><%= dataStudent.id %></td>
                        <td><%= dataStudent.firstName %></td>
                        <td><%= dataStudent.lastName %></td>
                        <td><%= dataStudent.email %></td>
                        <td><a href="/student/<%= dataStudent.id %>/addsubject">Add Subject</a></td>
                        <td><a href="/student/<%= dataStudent.id %>/editstudent">Edit</a> || <a href="/student/deletestudent/<%= dataStudent.id %>">Delete</a></td>
                    </tr>
                <%})%>
            </tbody>
        </table>
    </div>
</body>
</html>