<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Register Student</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
    <script src="main.js"></script>
    <%- include('../partials/style/tableStyles.ejs') %>
</head>
<body>
    <%- include('../partials/header/nav.ejs', {title: 'All Data Students', titleUrl: 'students/add', subTitle:'Add Student'}) %>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Actions</th>
                <th>Options</th>
            </tr>
        </thead>
        <tbody>
            <% dataStudents.forEach(dataStudent => { %>
                <tr>
                    <td><%= dataStudent.id %></td>
                    <td><%= dataStudent.firstName %></td>
                    <td><%= dataStudent.lastName %></td>
                    <td><%= dataStudent.email %></td>
                    <td><a href="/students/<%= dataStudent.id %>/add-subject">add subject</a></td>
                    <td><a href="/students/edit/<%= dataStudent.id %>">Edit</a> | <a href="/students/delete/<%= dataStudent.id %>">Delete</a></td>
                </tr>
         <%   }) %>
        </tbody>
    </table>
</body>
</html>